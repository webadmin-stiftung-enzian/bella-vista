{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1440px"
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#272727",
					"name": "dark gray",
					"slug": "dark-gray"
				},
				{
					"color": "#FFFFFF",
					"name": "white",
					"slug": "white"
				},
				{
					"color": "#F5F5F5",
					"name": "light gray",
					"slug": "light-gray"
				},
				{
					"color": "#000000",
					"name": "black",
					"slug": "black"
				}
			]
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"custom": {
			"lineHeight": {
				"xs": 1,
				"sm": 1.25,
				"md": 1.5,
				"lg": 1.75
			}
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "'Montserrat', sans-serif",
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontWeight": "300",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-300.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-400.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "500",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-500.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "600",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-600.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-700.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "800",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-800.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontWeight": "900",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/montserrat-900.woff2"
							]
						}
					]
				},
				{
					"fontFamily": "'PT Serif', serif",
					"name": "PT Serif",
					"slug": "pt-serif",
					"fontFace": [
						{
							"fontFamily": "PT Serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/pt-serif-400.woff2"
							]
						},
						{
							"fontFamily": "PT Serif",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/pt-serif-400-italic.woff2"
							]
						},
						{
							"fontFamily": "PT Serif",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/pt-serif-700.woff2"
							]
						},
						{
							"fontFamily": "PT Serif",
							"fontWeight": "700",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/pt-serif-700-italic.woff2"
							]
						}
					]
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"typography": {
			"fontSize": "1.2rem",
			"lineHeight": "1.25"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300",
					"fontSize": "4rem",
					"lineHeight": "1.25"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300",
					"lineHeight": "1.25"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300",
					"fontSize": "2rem",
					"lineHeight": "1.25"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300",
					"lineHeight": "1.25"
				}
			},
			"button": {
				"border": {
					"radius": "25px"
				},
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300"
				},
				"color": {
					"text": "#000000",
					"background": "#ffffff"
				}
			},
			"link": {
				"color": {
					"text": "#ffffff"
				},
				"typography": {
					"textDecoration": "none"
				},
				":hover": {
					"color": {
						"text": "#666666"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"blocks": {
			"core/group": {
				"color": {
					"text": "#000000",
					"link": "#777777"
				}
			},
			"core/button": {
				"border": {
					"radius": "25px"
				},
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300"
				},
				"color": {
					"text": "#000000",
					"background": "#ffffff"
				}
			},
			"core/heading": {
				"typography": {
					"fontFamily": "montserrat",
					"fontWeight": "300",
					"lineHeight": "md"
				}
			},
			"core/paragraph": {
				"typography": {
					"lineHeight": "lg",
					"fontFamily": "pt-serif",
					"fontWeight": "400"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}